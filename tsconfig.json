{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames":true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "moduleResolution": "Node",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "vite.config.ts"]
}
